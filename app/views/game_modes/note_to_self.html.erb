<form class="grid-x grid-margin-x" autocomplete="off">
  <div class="cell medium-12">
    <div>
      <label name="note-to-self">Match Notes</label>
      <textarea autocomplete="off" id="note-to-self" rows="4"><%= @note_to_self %></textarea>
    </div>
    <div>
      <button class="button cell success gm-update-note-to-self" value="submit">Save</button>
      <button class="button cell cancel gm-cancel-update" value="submit">Cancel</button>
    </div>
  </div>
</form>