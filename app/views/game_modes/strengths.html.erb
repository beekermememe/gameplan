<form class="grid-x grid-margin-x" autocomplete="off">
  <div class="cell medium-offset-2 medium-5">
    <div>
      <select class="edit-strengths" multiple size="10" heigth="100px">
        <% @all_strengths.each do |strength| %>
          <option value="<%= strength.id %>" <%= @current_strengths.include?(strength.id.to_s) ? "selected": "" %>><%= strength.title%></option>
        <% end %>
      </select>
    </div>
  </div>
  <div class="cell medium-offset-2 medium-5">
    <button class="button cell medium-offset-1 medium-4 success gm-update-strengths" value="submit">Save</button>
    <button class="button cell medium-offset-1 medium-4 cancel gm-cancel-update" value="submit">Cancel</button>
  </div>
</form>