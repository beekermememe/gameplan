<form>
  <div class="row">
    <div>
    <select multiple="multiple" size="10">
    <% @all_strengths.each do |strength| %>
      <option value="<%= strength.id %>" <%= @current_strengths.include?(strength.id.to_s) ? "selected": "" %>><%= strength.title%></option>
    <% end %>
    </select>
    </div>
  </div>
  <div class="row">
    <button class="success update-strengths" id="<%= @match.id%>" value="submit">Save</button>
    <button class="cancel cancel-update" id="<%= @match.id%>" value="submit">Cancel</button>
  </div>
</form>