<form class="grid-x grid-margin-x" autocomplete="off">
  <div class="cell medium-12">
    <div>
      <label name="team">League
        <input type="text" autocomplete="off" id="league-update" value="<%= @league %>">
      </label>
    </div>
    <div>
      <button class="button cell success update-league" id="<%= @match.id%>" value="submit">Save</button>
      <button class="button cell cancel cancel-update" id="<%= @match.id%>" value="submit">Cancel</button>
    </div>
  </div>
</form>